<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<!-- $Id$ -->
<plugin id="org.drftpd.plugins.jobmanager" version="2.1.0">
	<attributes>
		<attribute id="DenyUnload" value="false" />
		<attribute id="DefaultBuild" value="true" />
		<attribute id="MustBuild" value="false" />
	</attributes>
	<requires>
		<import plugin-id="master" exported="true" reverse-lookup="true" />
		<import plugin-id="org.drftpd.commandmanager" exported="true" />
	</requires>
	<runtime>
		<library id="org.drftpd.plugins.jobmanager" path="build/classes/" type="code">
			<export prefix="*" />
		</library>
	</runtime>
	<extension-point id="PostHook">
		<parameter-def id="HookClass" type="string" />
		<parameter-def id="HookMethod" type="string" />
		<parameter-def id="ParentMethod" type="string" />
		<parameter-def id="Priority" type="number" />
	</extension-point>
	<extension-point id="PreHook">
		<parameter-def id="HookClass" type="string" />
		<parameter-def id="HookMethod" type="string" />
		<parameter-def id="ParentMethod" type="string" />
		<parameter-def id="Priority" type="number" />
	</extension-point>
	<extension plugin-id="master" point-id="Plugin" id="JobManager">
		<parameter id="Class" value="org.drftpd.plugins.jobmanager.JobManager"/>
	</extension>
	<extension plugin-id="org.drftpd.commandmanager" point-id="Command" id="JobManagerCommandHandler" />
</plugin>
