	</attributes>
	<runtime>
		<library id="common" path="classes/" type="code">
			<export prefix="*" />
		</library>
		<library id="junit" path="../../lib/junit-4.3.1.jar" type="code">
			<export prefix="*" />
		</library>
		<library id="log4j" path="../../lib/log4j-1.2.14.jar" type="code">
			<export prefix="*" />
		</library>
		<library id="oro" path="../../lib/oro.jar" type="code">
			<export prefix="*" />
		</library>
	</runtime>
</plugin>
